{
    "manifest_version": 3,
    "name": "Whack-a-Mole Game",
    "version": "1.0",
    "description": "A fun Whack-a-Mole game to play in your browser.",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "permissions": [
      "activeTab"
    ]
  }
  